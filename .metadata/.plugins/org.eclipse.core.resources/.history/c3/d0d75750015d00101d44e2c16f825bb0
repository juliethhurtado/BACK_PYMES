    @Override
    public List<EdificioOutDTO> toLstEdificioOutDTO(List<Edificio> lstEdificio) {
        if ( lstEdificio == null ) {
            return null;
        }

        List<EdificioOutDTO> list = new ArrayList<EdificioOutDTO>( lstEdificio.size() );
        for ( Edificio edificio : lstEdificio ) {
            list.add( toEdificioOutDTO( edificio ) );
